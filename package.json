{
  "name": "birthday-countdown-js",
  "version": "5.0.0",
  "description": "birthday countdown javascript",
  "main": "none",
  "scripts": {
    "test": "eslint src/countdown.ts",
    "build": "tsc -b",
    "watch": "tsc -w",
    "serve": "browser-sync start --serveStatic 'src/' --server --files 'build/*.js, src/index.html'",
    "format": "prettier --write \"src/*.{js,ts}\"",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "author": "yamanoku",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "browser-sync": "2.27.7",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "lint-staged": "12.3.1",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "lint-staged": {
    "src/*.{js,ts}": [
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamanoku/birthday-countdown-js.git"
  },
  "keywords": [
    "birthday",
    "countdown",
    "ES2017",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamanoku/birthday-countdown-js/issues"
  },
  "homepage": "https://github.com/yamanoku/birthday-countdown-js#readme"
}
