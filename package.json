{
  "name": "yamanoku-birthday-countdown",
  "version": "7.0.0",
  "description": "yamanoku birthday countdown app",
  "main": "none",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode client && vite build",
    "preview": "npx wrangler pages dev ./dist",
    "deploy": "$npm_execpath run build && npx wrangler pages deploy ./dist",
    "biome": "biome check --write .",
    "biome:ci": "biome ci .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "hono": "^4.2.3",
    "honox": "^0.1.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@cloudflare/workers-types": "4.20250927.0",
    "@hono/vite-cloudflare-pages": "^0.4.0",
    "vite": "^7.0.0",
    "vitest": "^3.1.2",
    "wrangler": "^4.0.0"
  },
  "author": "yamanoku",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamanoku/birthday-countdown-js.git"
  },
  "keywords": [
    "birthday",
    "countdown",
    "typescript",
    "Hono",
    "HonoX"
  ],
  "bugs": {
    "url": "https://github.com/yamanoku/birthday-countdown-js/issues"
  },
  "homepage": "https://github.com/yamanoku/birthday-countdown-js#readme"
}
