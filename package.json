{
  "name": "birthday-countdown-js",
  "version": "3.1.0",
  "description": "birthday countdown javascript",
  "main": "none",
  "scripts": {
    "lint": "eslint src/*.js && tslint src/ts/*.ts; exit 0",
    "build": "babel src -d build",
    "tsc": "tsc",
    "serve": "browser-sync start --serveStatic 'src/' --server --files 'src/*.js, src/index.html'",
    "precommit": "lint-staged",
    "postmerge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && yarn || :"
  },
  "author": "yamanoku",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "browser-sync": "2.26.3",
    "eslint": "4.19.1",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "tsc": "1.20150623.0",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "test"
    ],
    "src/ts/*.ts": [
      "test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamanoku/birthday-countdown-js.git"
  },
  "keywords": [
    "birthday",
    "countdown",
    "ES2015",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamanoku/birthday-countdown-js/issues"
  },
  "homepage": "https://github.com/yamanoku/birthday-countdown-js#readme",
  "dependencies": {
    "global": "^4.3.2"
  }
}
