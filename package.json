{
  "name": "birthday-countdown-js",
  "version": "4.1.0",
  "description": "birthday countdown javascript",
  "main": "none",
  "scripts": {
    "lint": "eslint src/countdown.ts",
    "build": "tsc -b",
    "watch": "tsc -w",
    "serve": "browser-sync start --serveStatic 'src/' --server --files 'build/*.js, src/index.html'",
    "prettier": "prettier **/*.{ts,js}",
    "prepare": "husky install"
  },
  "author": "yamanoku",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "browser-sync": "2.27.4",
    "eslint": "7.30.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "7.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "src/*.ts": [
      "lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yamanoku/birthday-countdown-js.git"
  },
  "keywords": [
    "birthday",
    "countdown",
    "ES2015",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/yamanoku/birthday-countdown-js/issues"
  },
  "homepage": "https://github.com/yamanoku/birthday-countdown-js#readme"
}
